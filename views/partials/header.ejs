    <script type="text/javascript">
         function getTimestamp() {
            var timestamp = Date.now();
            var date = new Date(timestamp);
            var commentDate = {
              y : date.getFullYear().toString(),
              mo : (date.getMonth()+1).toString(),  //month starts at 0
              d : date.getDate().toString(),
              h : date.getHours().toString(),
              m : addZero(date.getMinutes()).toString(),
              s : addZero(date.getSeconds()).toString()
            };
        
             var d = commentDate.mo+"/"+ commentDate.d+"/"+commentDate.y+", "+commentDate.h+":"+commentDate.m;
            
             return d;
        }
        function addZero(i) {
            if (i < 10) {
                i = "0" + i;
            }
            return i;
        }
        
    </script>    
    
